generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int    @id
  firstName  String
  lastName   String
  age        Int
  gender     String
  hairColor  String
  postalCode String
  department String
}

model DepartmentSummary {
  departmentName String @id
  maleCount      Int
  femaleCount    Int
  ageMin         Int
  ageMax         Int
  hairSummary    Json
  addressUsers   Json
}

